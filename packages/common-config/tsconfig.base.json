{
    "compilerOptions": {
        "lib": [
            "ESNext"
        ],
        "moduleResolution": "nodenext",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es6",
        "module": "commonjs",
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}
